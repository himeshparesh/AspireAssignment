{
  "name": "aspire",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "pod": "npx pod-install",
    "a": "react-native run-android",
    "ios": "react-native run-ios",
    "i": "react-native run-ios --device 'Sodainmindâ€™s iPhone'",
    "images": "node scripts/images.js",
    "unlink-ios": "react-native-asset -ios-a",
    "unlink-android": "react-native-asset -android-a",
    "link": "react-native-asset",
    "ios-dev-bundle": "react-native bundle --entry-file index.js --platform ios --bundle-output ios/main.jsbundle",
    "android-dev-bundle": "react-native bundle --entry-file index.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "android-bundle": "react-native bundle --dev false --entry-file index.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/bundle",
    "clear-cache": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && npm cache clean --force && npm install && npm start -- --reset-cache"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.17",
    "axios": "^0.25.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "moment": "^2.29.1",
    "react": "17.0.2",
    "react-native": "0.67.1",
    "react-native-gesture-handler": "^2.2.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-reanimated": "2.2.4",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.2",
    "react-native-smart-tip": "^2.3.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-ui-lib": "^6.8.1",
    "react-navigation": "^4.4.4",
    "react-navigation-redux-helpers": "^4.0.1",
    "react-navigation-stack": "^2.10.4",
    "react-navigation-tabs": "^2.11.1",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "@types/react-native": "^0.66.15",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "typescript": "^4.5.5"
  },
  "jest": {
    "preset": "react-native"
  }
